<scenefile>
  <globaldata>
    <diffusecoeff v="1"/>
    <specularcoeff v="0.5"/>
    <ambientcoeff v="1"/>
  </globaldata>

  <cameradata>
    <pos x="0" y="5" z="0"/>
    <look x="-.7" y="0" z=".7"/>
    <up x="0" y="1" z="0"/>
    <heightangle v="45"/>
  </cameradata>

  <lightdata>
    <id v="1"/>
    <position x="0" y="10" z="0"/>
    <color r="1" g="1" b="1"/>
  </lightdata>

  <object type="tree" name="lamp">
    <transblock>
      <object type="primitive" name="sphere">
        <diffuse r="1" g=".5" b="0"/>
      </object>
    </transblock>
  </object>

