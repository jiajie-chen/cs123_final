    </transblock>
  </object>

  <object type="tree" name="floorTile">
    <transblock>
      <translate x=".5" y="0" z=".5"/>
      <object type="primitive" name="cube">
        <diffuse r="0.6" g="0.6" b="0.6"/>
        <ambient r="0.3" g="0.3" b="0.3"/>
        <specular r="0" g="0" b="0"/>
        <texture file="../textures/tex/marble.png" u="4" v="4"/>
        <bumpmap file="../textures/bump/seamless_dirt_normal.jpg" u="4" v="4"/>
