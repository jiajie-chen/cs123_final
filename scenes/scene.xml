    </transblock>
  </object>

  <object type="tree" name="skyBox">
  	<transblock>
  		<scale x="-1" y="1" z="1"/>
  		<object type="primitive" name="cube">
  			<diffuse r="1" g="1" b="1"/>
  			<ambient r="1" g="1" b="1"/>
  			<specular r="0" g="0" b="0"/>
  			<texture file="../textures/tex/skybox.jpg" u="4" v="4"/>
  		</object>
