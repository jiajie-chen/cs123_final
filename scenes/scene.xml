        <texture file="leaf.png" u="1" v="1"/>
        <bumpmap file="bark.png" u="1" v="1"/>
      </object>
    </transblock>
    <transblock>
      <translate x="0" y="0" z="10"/>
      <object type="primitive" name="sphere" >
        <diffuse r=".2" g="1" b=".4"/>
        <texture file="leaf.png" u="1" v="1"/>
        <bumpmap file="bark.png" u="1" v="1"/>
      </object>
    </transblock>
    <transblock>
      <translate x="0" y="0" z="20"/>
      <object type="primitive" name="sphere" >
        <diffuse r=".2" g="1" b=".4"/>
        <texture file="leaf.png" u="1" v="1"/>
