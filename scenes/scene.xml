  <object type="tree" name="root">
    <transblock>
      <object type="master" name="room"/>
    </transblock>
    <transblock>
      <translate x="-50" y="0" z="0"/>
      <rotate x="0" y="1" z="0" angle="180"/>
      <object type="master" name="room"/>
    </transblock>
    <transblock>
      <translate x="-50" y="0" z="0"/>
      <rotate x="0" y="1" z="0" angle="180"/>
      <translate x="25" y="25" z="0"/>
      <scale x="1" y="50" z="100"/>
      <object type="primitive" name="cube" >
        <diffuse r="1" g="1" b="1"/>
        <texture file="wall.png" u="1" v="1"/>
        <bumpmap file="bark.png" u="1" v="1"/>
      </object>
    </transblock>
  </object>


</scenefile>

