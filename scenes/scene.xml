        <texture file="leaf.png" u="1" v="1"/>
        <bumpmap file="bark.png" u="1" v="1"/>
      </object>
    </transblock>
    <transblock>
      <translate x="0" y="0" z="40"/>
      <object type="primitive" name="sphere" >
        <diffuse r=".2" g="1" b=".4"/>
        <texture file="leaf.png" u="1" v="1"/>
        <bumpmap file="bark.png" u="1" v="1"/>
      </object>
    </transblock>
    <transblock>
      <translate x="0" y="0" z="35"/>
      <object type="master" name="lamp"/>
    </transblock>
  </object>

  <object type="tree" name="room">
    <transblock>
      <scale x="50" y="1" z="100"/>
