        <bumpmap file="bark.png" u="1" v="1"/>
      </object>
    </transblock>
    <transblock>
      <translate x="0" y="1" z="0"/>
      <object type="tree">
        <transblock>
          <object type="master" name="row"/>
        </transblock>
        <transblock>
          <translate x="-10" y="0" z="0"/>
          <object type="master" name="row"/>
        </transblock>
        <transblock>
          <translate x="-20" y="0" z="0"/>
