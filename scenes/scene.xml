    </transblock>
    <transblock>
      <translate x="0" y="-.5" z="-.5"/>
      <object type="primitive" name="cube">
        <diffuse r="1" g="1" b="1"/>
        <texture file="../textures/tex/wall.jpg" u="4" v="4"/>
        <bumpmap file="../textures/bump/wall.png" u="4" v="4"/>
